{
    "manifest_version": 3,
    "name": "Github Folder Downloader",
    "description": "Download Folder from Github",
    "version": "1.0",
    "content_scripts":[
      {
        "matches":["<all_urls>"],
        "js":["content-script.js"]
      }
    ],
    "background":{
      "service_worker":"background.js"
    },
    "permissions":["tabs"],
    "action":{},
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }